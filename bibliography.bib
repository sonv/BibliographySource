	file = {:http\://arxiv.org/pdf/2101.10287v2:PDF},
	journal = {SIAM Journal of Mathematical Analysis},
	keywords = {math.AP, math.PR},
	title = {Bounds on the heat transfer rate via passive advection},
	year = {2021}}

